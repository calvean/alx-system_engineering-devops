server {
    listen      80 default_server;
    listen      [::]:80 default_server ipv6only=on;
    server_name 54.237.46.86;
    add_header  X-Served-By 67841-web-01;

    location / {
        proxy_pass http://unix:/home/flask/flask.sock;
    }

    error_page 404 /404.html;
    location /404 {
        root /var/www/html;
        internal;
    }
}

